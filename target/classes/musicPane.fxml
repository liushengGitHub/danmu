<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXSlider?>
<HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      fx:controller="liusheng.danmu.controller.MusicPaneController"
      prefHeight="40.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXButton mnemonicParsing="false" fx:id="playButton" prefHeight="40.0" prefWidth="40.0"  >
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </JFXButton>
      <JFXButton mnemonicParsing="false" fx:id="nextButton" prefHeight="40.0" prefWidth="40.0">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </JFXButton>
      <VBox prefHeight="40.0" prefWidth="400.0">

         <children>

            <JFXSlider prefHeight="20.0" prefWidth="400.0" fx:id="progressSlid" max="100" min="0" value="0" />
            <Label prefHeight="20.0" prefWidth="400.0" fx:id="songName"  text="暂无名字"/>
         </children>
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </VBox>
      <Label prefHeight="40.0" prefWidth="40.0" fx:id="songSlid">

      </Label>
   </children>
</HBox>
